#!/usr/bin/env sh

data=$(date '+%Y-%m-%d %H:%M:%S')

echo "= .xprofile ========================= $data" >> "$LOGINLOG"

mpd &
xplugd >> "$LOGINLOG" 2>&1 &
~/.fehbg >> "$LOGINLOG" 2>&1 &
syncthing -no-browser &
dunst >> "$LOGINLOG" 2>&1 &
picom >> "$LOGINLOG" 2>&1 &
overgrive >> "$LOGINLOG" 2>&1 &
transmission-daemon >> "$LOGINLOG" 2>&1 &
xscreensaver -nosplash &
mpdas -c ~/.config/mpdasrc >> "$LOGINLOG" 2>&1 &
unclutter &
emacs --daemon &
touchegg &

export MONITOR1=$(xrandr -q | grep connected | grep -v disconnected | awk 'FNR == 1 {print $1}')
# export MONITOR2=$(xrandr -q | grep connected | grep -v disconnected | awk 'FNR == 2 {print $1}')

